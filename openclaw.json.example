      "kimi-claw": {
        "_comment": "Kimi Claw Extension - Bridge to Kimi AI",
        "enabled": true,
        "config": {
          "bridge": {
            "_comment": "Bridge authentication token - KEEP SECRET",
            "_comment": "Generate via: openclaw bridge token create",
            "token": "YOUR_BRIDGE_TOKEN_HERE",
            "promptTimeoutMs": 1800000
          }
        }
      },
      "telegram": {
        "_comment": "Telegram Bot Integration",
        "enabled": true
      }
