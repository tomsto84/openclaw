      "mode": "token",
      "token": "YOUR_GATEWAY_TOKEN_HERE"
    },
    "port": 18789,
    "bind": "loopback",
    "tailscale": {
      "_comment": "Tailscale VPN integration (optional)",
      "mode": "off",
      "resetOnExit": false
    }
